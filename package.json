{
  "name": "stg-track-my-case-ui",
  "version": "1.0.0",
  "description": "[![repo standards badge](https://img.shields.io/endpoint?labelColor=231f20&color=005ea5&style=for-the-badge&label=MoJ%20Compliant&url=https%3A%2F%2Foperations-engineering-reports.cloud-platform.service.justice.gov.uk%2Fapi%2Fv1%2Fcompliant_public_repositories%2Fendpoint%2Ftemplate-repository&logo=data:image/png;base64,)](https://operations-engineering-reports.cloud-platform.service.justice.gov.uk/public-github-repositories.html#template-repository)",
  "main": "index.js",
  "scripts": {
    "watch-ts": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node $NODE_OPTIONS dist/server.js",
    "start:dev": "concurrently -k -p \"[{name}]\" -n \"ESBuild,Node\" -c \"yellow.bold,cyan.bold\" \"node esbuild/esbuild.config.js --build --watch\" \"node esbuild/esbuild.config.js --dev-server\"",
    "build": "node esbuild/esbuild.config.js --build",
    "typecheck": "tsc",
    "clean": "rm -rf dist node_modules",
    "rebuild": "npm run clean && npm i && npm run build"
  },
  "engines": {
    "node": "^22",
    "npm": "^10"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^5.1.0",
    "express": "^4.21.2",
    "govuk-frontend": "^5.9.0"
  },
  "devDependencies": {
    "@jgoz/esbuild-plugin-typecheck": "^4.0.3",
    "@tsconfig/node22": "^22.0.1",
    "@types/connect-flash": "^0.0.40",
    "@types/express-session": "^1.18.1",
    "@types/node": "^22.14.0",
    "@types/nunjucks": "^3.2.6",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.2",
    "esbuild-plugin-clean": "^1.0.1",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-manifest": "^1.0.5",
    "esbuild-sass-plugin": "^3.3.1",
    "glob": "^11.0.1",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "@jgoz/esbuild-plugin-typecheck": {
      "esbuild": "$esbuild"
    }
  }
}
